{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "targets": {
                    "ie": "11"
                }
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-external-helpers",
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": { "version": 3, "proposals": true },
                "useESModules": true
            }
        ]
    ]
}
