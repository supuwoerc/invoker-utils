{
    "name": "@supuwoerc/utils",
    "version": "1.1.5",
    "description": "前端工具方法",
    "types": "./dist/index.d.ts",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.esm.js",
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "require": "./dist/index.cjs.js",
            "default": "./dist/index.umd.js",
            "types": "./dist/index.d.ts"
        },
        "./dist/index.esm.js": {
            "import": "./dist/index.esm.js",
            "types": "./dist/index.d.ts"
        },
        "./dist/index.cjs.js": {
            "require": "./dist/index.cjs.js",
            "types": "./dist/index.d.ts"
        },
        "./dist/index.umd.js": {
            "default": "./dist/index.umd.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "prepare": "husky install",
        "build": "rollup -c && npm run doc",
        "build:tsc": "npx tsc",
        "dev": "rollup -c --watch",
        "doc": "typedoc --options typedoc.json",
        "test": "jest --watch",
        "test:commit": "jest",
        "coverage": "jest --coverage",
        "lint": "eslint --ext .ts src --config=.eslintrc.json",
        "lint:fix": "eslint --ext .ts src --fix --config=.eslintrc.json",
        "lint-staged": "lint-staged",
        "commitlint": "commitlint --config commitlint.config.cjs -e -V",
        "publish:public": "npm publish --access public"
    },
    "lint-staged": {
        "src/**/*.ts": "npm run lint"
    },
    "files": [
        "dist",
        "*.d.ts"
    ],
    "keywords": [
        "utils",
        "front-end-utils",
        "supuwoerc-utils",
        "typescript",
        "javascript"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/supuwoerc/supuwoerc-utils.git"
    },
    "author": "QiMeng Zhang <zhangzhouou@gmail.com>(https://github.com/supuwoerc)",
    "homepage": "https://supuwoerc.github.io/supuwoerc-utils",
    "bugs": {
        "url": "https://github.com/supuwoerc/supuwoerc-utils/issues"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.21.8",
        "@babel/preset-env": "^7.24.0",
        "@commitlint/cli": "^17.6.3",
        "@commitlint/config-conventional": "^17.6.3",
        "@rollup/plugin-alias": "^5.0.0",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^24.1.0",
        "@rollup/plugin-eslint": "^9.0.3",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@types/jest": "^29.5.1",
        "@types/throttle-debounce": "^5.0.0",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "@typescript-eslint/parser": "^5.59.2",
        "babel-preset-env": "^1.7.0",
        "esbuild": "^0.17.18",
        "eslint": "^8.40.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-custom-alias": "^1.3.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-canvas-mock": "^2.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "lint-staged": "^13.2.2",
        "prettier": "^2.8.8",
        "rollup": "^3.21.5",
        "rollup-plugin-cleaner": "^1.0.0",
        "rollup-plugin-dts": "^5.3.0",
        "rollup-plugin-esbuild": "^5.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.34.1",
        "ts-jest": "^29.1.0",
        "typedoc": "^0.24.6",
        "typescript": "^5.0.4"
    },
    "volta": {
        "node": "16.14.2"
    },
    "dependencies": {
        "@babel/plugin-external-helpers": "^7.23.3",
        "@babel/plugin-transform-runtime": "^7.24.0",
        "@babel/runtime": "^7.24.0",
        "@babel/runtime-corejs3": "^7.24.0",
        "throttle-debounce": "^5.0.0"
    }
}
